{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "title": {
    "text": "Unemployment in France (1975-2023)",
    "anchor": "start",
    "subtitle": "Rate, % | Source: Insee through ECO API",
    "font": "Manrope",
    "fontSize": 14,
    "subtitleFontSize": 12,
    "subtitleFont": "Manrope",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "white",
  "view": {"fill": "white", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/fra/unem?vega",
    "format": {
      "type": "json"
    }
  },
  "mark": {
    "type": "line",
    "color": "#FF5020",
    "strokeWidth": 1
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "title": "Years",
        "titleColor": "#000000",
        "grid": true,
        "titleFontSize": 12,
        "titleFont": "Manrope",
        "labelFont": "Manrope",
        "gridColor": "#d7d7d7",
        "gridOpacity": 0.4,
        "gridDash": [2, 2]
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "% Unemployment",
        "titleColor": "#000000",
        "grid": true,
        "titleFontSize": 12,
        "titleFont": "Manrope",
        "labelFont": "Manrope",
        "gridColor": "#d7d7d7",
        "gridOpacity": 0.4,
        "gridDash": [2, 2]
        
      }
    }
  }
}